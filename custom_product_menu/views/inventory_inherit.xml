<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_consumed_product" model="ir.actions.act_window">
            <field name="name">Consumed Material</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids"
                   eval="[(5, 0, 0),
                          (0, 0, {'view_mode': 'tree', 'view_id': ref('product.product_template_tree_view')}),
                          (0, 0, {'view_mode': 'form', 'view_id': ref('product.product_template_only_form_view')})]"/>
            <field name="context">{}</field>
            <field name="domain">['|','|','|',('categ_id.name','=','Compaign'),('categ_id.parent_id.name','=','Liquid'),('categ_id.name','=','Compaign'),('categ_id.parent_id.name','=','Solid')]</field>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">

              </p>
            </field>
        </record>

    <menuitem id="new_raw_material_menu" name="Raw Material Compagin"  parent="stock.menu_stock_inventory_control" sequence="12" action="action_consumed_product"/>

</odoo>